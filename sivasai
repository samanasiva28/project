pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }

        stage('Build') {
            steps {
                echo 'Thanks for coming here'
            }
        }

        stage('Test') {
            steps {
                echo 'Running test cases...'
                echo 'All tests passed!'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying the application...'
                echo 'Deployment successful!'
            }
        }

        stage('Cleanup') {
            steps {
                echo 'Cleaning up workspace...'
                echo 'Cleanup complete!'
            }
        }
    }
}
